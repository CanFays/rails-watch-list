<h1>All my lists:</h1>
<ul>
  <% @lists.each do |list| %>
    <li> <%= link_to list.name, list_path(list) %></li>
      <%= link_to "Delete",
      list_path(list),
      data: {turbo_method: :delete, turbo_confirm: "Are you sure?"} %>

  <% end %>
</ul>

<p><%=link_to "Create new list", new_list_path  %></p>
